<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree>
    <RetryUntilSuccessful num_attempts="-1">
      <ForceFailure>
        <Sequence>
          <Fallback>
            <Inverter>
              <IsBatteryLow min_battery="50.00" battery_topic="/talosbot1/battery_level_2"/>
            </Inverter>
            <ForceFailure>
              <TalosbotNavigateToPose action_name="/navigate_to_pose" goal_x="3.79" goal_y="4.58" quat_w="0.78" quat_z="-0.619"/>
            </ForceFailure>
          </Fallback>
          <Sequence>
            <TalosbotLockingPin action_name="/talosbot_locking_pin" up_position="false" />
            <TalosbotNavigateToPose action_name="/navigate_to_pose" goal_x="-4.57" goal_y="0.04" quat_w="-0.03" quat_z="0.99"/>
            <TalosbotGoUnderTrolley action_name="/talosbot_go_under_trolley" go="true"/>
            <TalosbotLockingPin action_name="/talosbot_locking_pin" up_position="true" />
            <TalosbotNavigateToPose action_name="/navigate_to_pose" goal_x="-8.35" goal_y="0.04" quat_w="-0.03" quat_z="0.99"/>
            <TalosbotNavigateToPose action_name="/navigate_to_pose" goal_x="-6.08" goal_y="-4.54" quat_w="0.80" quat_z="0.593"/>
            <TalosbotLockingPin action_name="/talosbot_locking_pin" up_position="false" />
            <TalosbotNavigateToPose action_name="/navigate_to_pose" goal_x="0.00" goal_y="0.00" quat_w="-0.1" quat_z="0.99"/>

            <!-- <TalosbotLockingPin action_name="/talosbot_locking_pin" up_position="true" /> -->
            <!-- <TalosbotNavigateToPose action_name="/navigate_to_pose" goal_x="-3.91" goal_y="6.09" quat_w="0.77" quat_z="-0.63"/> -->
          </Sequence>
        </Sequence>
      </ForceFailure>
    </RetryUntilSuccessful>
  </BehaviorTree>
</root>
